{
    "name": "evandotpro/edp-github",
    "description": "Github API integration module for Zend Framework 2",
    "type": "zf-module",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "module",
        "github",
        "api"
    ],
    "homepage": "https://github.com/EvanDotPro/EdpGithub",
    "authors": [
        {
            "name": "Evan Coury",
            "email": "me@evancoury.com",
            "homepage": "http://blog.evan.pro/"
        },
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com",
            "homepage": "http://hounddog.github.io"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-eventmanager": "~2.5",
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-http": "~2.5",
        "zendframework/zend-filter": "~2.5"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.4",
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": ">=0.6.0"
    },
    "autoload": {
        "psr-4": {
            "EdpGithub\\": "src/EdpGithub"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
